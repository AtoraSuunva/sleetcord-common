{
  "name": "sleetcord-common",
  "version": "14.0.0",
  "description": "Common modules/utils for sleetcord bots",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "biome check && tsc --noEmit",
    "lint:fix": "biome check --write && tsc --noEmit",
    "prepublishOnly": "pnpm run lint && pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtoraSuunva/sleetcord-common.git"
  },
  "keywords": [],
  "author": "AtoraSuunva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtoraSuunva/sleetcord-common/issues"
  },
  "homepage": "https://github.com/AtoraSuunva/sleetcord-common#readme",
  "dependencies": {
    "@sentry/node": "^9.19.0",
    "env-var": "^7.5.0",
    "pino": "^9.6.0",
    "pretty-ms": "^9.2.0"
  },
  "peerDependencies": {
    "@prisma/client": "^6.7.0",
    "discord.js": "^14.17.2",
    "sleetcord": "^2.3.0"
  },
  "peerDependenciesMeta": {
    "@prisma/client": {
      "optional": true
    }
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.15.18",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
