{
  "name": "sleetcord-common",
  "version": "2.3.0",
  "description": "Common modules/utils for sleetcord bots",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/ && prettier --check src/ && tsc --noEmit",
    "lint:fix": "eslint src/ --fix && prettier --write src/ && tsc --noEmit",
    "start:dev": "node --loader ts-node/esm -r dotenv/config test/index.ts",
    "prepublishOnly": "pnpm run lint && pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtoraSuunva/sleetcord-common.git"
  },
  "keywords": [],
  "author": "AtoraSuunva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtoraSuunva/sleetcord-common/issues"
  },
  "homepage": "https://github.com/AtoraSuunva/sleetcord-common#readme",
  "peerDependencies": {
    "discord.js": "^14.13.0",
    "env-var": "^7.3.1",
    "pino": "^8.14.2",
    "pretty-ms": "^8.0.0",
    "rollbar": "^2.26.2",
    "sleetcord": "^0.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "prettier": "^3.0.2",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  }
}
