{
  "name": "sleetcord-common",
  "version": "7.0.0",
  "description": "Common modules/utils for sleetcord bots",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/ && prettier --check src/ && tsc --noEmit",
    "lint:fix": "eslint src/ --fix && prettier --write src/ && tsc --noEmit",
    "prepublishOnly": "pnpm run lint && pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtoraSuunva/sleetcord-common.git"
  },
  "keywords": [],
  "author": "AtoraSuunva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtoraSuunva/sleetcord-common/issues"
  },
  "homepage": "https://github.com/AtoraSuunva/sleetcord-common#readme",
  "dependencies": {
    "@prisma/client": "^5.13.0",
    "@sentry/integrations": "^7.113.0",
    "@sentry/node": "^7.113.0",
    "@sentry/profiling-node": "^7.113.0",
    "discord.js": "^14.14.1",
    "env-var": "^7.4.1",
    "pino": "^9.0.0",
    "pretty-ms": "^9.0.0",
    "sleetcord": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}
